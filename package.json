{
  "name": "LOL-Esports-Prediction-Bot",
  "version": "1.0.0",
  "description": "Discord bot for LOL esports match predictions",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js"
      async getMatchPredictions(matchId) {
        return new Promise((resolve, reject) => {
            this.db.all(
                'SELECT * FROM predictions WHERE match_id = ?',
                [matchId],
                (err, rows) => {
                    if (err) reject(err);
                    else resolve(rows || []);
                }
            );
        });
    },
  "dependencies": {
    "discord.js": "^14.14.1",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "node-cron": "^3.0.3",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": ["discord", "bot", "lol", "esports", "prediction"],
  "author": "Crazyfighting",
  "license": "MIT"
}